{
    "Serilog": {
        "Using": [
            "Serilog.Sinks.Seq",
            "Serilog.Sinks.File",
            "Serilog.Sinks.Console",
            "Serilog.Enrichers.HttpContext"
        ],
        "MinimumLevel": {
            "Default": "Information",
            "Override": {
                "Microsoft": "Warning",
                "System": "Warning"
            }
        },
        "Enrich": [
            "FromLogContext",
            "WithEnvironmentName",
            "WithThreadId",
            "WithClientIp",
            "WithRequestBody",
            "WithRequestQuery",
            {
                "Name": "WithCorrelationId",
                "Args": {
                    "headerName": "correlation-id",
                    "addValueIfHeaderAbsence": true
                }
            },
            {
                "Name": "WithRequestHeader",
                "Args": {
                    "headerName": "User-Agent"
                }
            },
            {
                "Name": "WithRequestHeader",
                "Args": {
                    "headerName": "Connection"
                }
            },
            {
                "Name": "WithRequestHeader",
                "Args": {
                    "headerName": "Content-Length",
                    "propertyName": "RequestLength"
                }
            }
        ],
        "WriteTo": [
            {
                "Name": "Console",
                "Args": {
                    "formatter": {
                        "type": "Serilog.Formatting.Compact.CompactJsonFormatter, Serilog.Formatting.Compact"
                    }
                }
            },
            {
                "Name": "File",
                "Args": {
                    "path": "Logs/logs_.txt",
                    "restrictedToMinimumLevel": "Information",
                    "rollingInterval": "Day",
                    "retainedFileCountLimit": 7,
                    "formatter": {
                        "type": "Serilog.Formatting.Json.JsonFormatter, Serilog",
                        "renderMessage": true
                    }
                }
            },
            {
                "Name": "File",
                "Args": {
                    "path": "Logs/errors_.txt",
                    "restrictedToMinimumLevel": "Error",
                    "rollingInterval": "Day",
                    "retainedFileCountLimit": 7,
                    "formatter": {
                        "type": "Serilog.Formatting.Json.JsonFormatter, Serilog",
                        "renderMessage": true
                    }
                }
            },
            {
                "Name": "Sentry",
                "Args": {
                    "Dsn": "https://046428995641391d8c294e3ad7487e55@o4508670634557440.ingest.de.sentry.io/4508670710841424",
                    "SendDefaultPii": true,
                    "MaxRequestBodySize": "Always",
                    "MinimumBreadcrumbLevel": "Debug",
                    "MinimumEventLevel": "Warning",
                    "AttachStackTrace": true,
                    "Debug": true,
                    "DiagnosticLevel": "Error",
                    "TracesSampleRate": 1.0
                }
            }
        ]
    },
    "SyncfusionKey": "Syncfusion-Key"
}