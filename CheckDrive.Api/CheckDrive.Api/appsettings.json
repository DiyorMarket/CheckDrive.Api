{
    "Serilog": {
        "Using": [ "Serilog.Sinks.File", "Serilog.Sinks.Console" ],
        "MinimumLevel": {
            "Default": "Information",
            "Override": {
                "Microsoft": "Warning",
                "System": "Warning"
            }
        },
        "Enrich": [ "FromLogContext", "WithEnvironmentName", "WithThreadId" ],
        "WriteTo": [
            {
                "Name": "Console",
                "Args": {
                    "formatter": {
                        "type": "Serilog.Formatting.Json.JsonFormatter, Serilog",
                        "renderMessage": true
                    }
                }
            },
            {
                "Name": "File",
                "Args": {
                    "path": "Logs/logs_.txt",
                    "restrictedToMinimumLevel": "Information",
                    "rollingInterval": "Day",
                    "retainedFileCountLimit": 7,
                    "formatter": {
                        "type": "Serilog.Formatting.Json.JsonFormatter, Serilog",
                        "renderMessage": true
                    }
                }
            },
            {
                "Name": "File",
                "Args": {
                    "path": "Logs/errors_.txt",
                    "restrictedToMinimumLevel": "Error",
                    "rollingInterval": "Day",
                    "retainedFileCountLimit": 7,
                    "formatter": {
                        "type": "Serilog.Formatting.Json.JsonFormatter, Serilog",
                        "renderMessage": true
                    }
                }
            }
        ]
    },
    "SyncfusionKey": "key"
}